{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@fixture": ["./tests/fixtures/fixture.ts"],
      "@testData/*": ["./test-data/*"],
      "@selectors": ["./selectors/selectors.json"],
      "@stepRunner": ["./src/lib/test-step-runner.ts"],
      "@pages/*": ["./src/pages/*"]
    }
  }
}
